@model IEnumerable<DataAbstraction.Models.YearView.YearViewModel>
<div class="container" style="margin-left:20%;">
<table class="table table-hover table-sm">
    <thead>
        <!-- style="position: sticky; top: 45px;background: white;"-->
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SecCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <!--<th>
                @Html.DisplayNameFor(model => model.ISIN)
            </th>-->
            <th>
                @Html.DisplayNameFor(model => model.Pieces)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AvPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Volume)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feb)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mar)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.May)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jun)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jul)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aug)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sep)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Okt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nov)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dec)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Summ)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VolPercent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DivPercent)
            </th>
            <!--<th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>-->
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SecCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <!--<td>
            @Html.DisplayFor(modelItem => item.ISIN)
        </td>-->
        <td>
            @Html.DisplayFor(modelItem => item.Pieces)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AvPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Volume)
        </td>
                @{
                    if (item.Jan != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Февраль">
                                @Html.DisplayFor(modelItem => item.Jan)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Jan)
                            </td>
                    }

                    if (item.Feb != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Февраль">
                                @Html.DisplayFor(modelItem => item.Feb)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Feb)
                            </td>
                    }

                    if (item.Mar != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Март">
                                @Html.DisplayFor(modelItem => item.Mar)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Mar)
                            </td>
                    }

                    if (item.Apr != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Апрель">
                                @Html.DisplayFor(modelItem => item.Apr)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Apr)
                            </td>
                    }

                    if (item.May != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Май">
                                @Html.DisplayFor(modelItem => item.May)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.May)
                            </td>
                    }

                    if (item.Jun != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Июнь">
                                @Html.DisplayFor(modelItem => item.Jun)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Jun)
                            </td>
                    }

                    if (item.Jul != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Июль">
                                @Html.DisplayFor(modelItem => item.Jul)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Jul)
                            </td>
                    }

                    if (item.Aug != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Август">
                                @Html.DisplayFor(modelItem => item.Aug)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Aug)
                            </td>
                    }

                    if (item.Sep != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Сентябрь">
                                @Html.DisplayFor(modelItem => item.Sep)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Sep)
                            </td>
                    }

                    if (item.Okt != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Октябрь">
                                @Html.DisplayFor(modelItem => item.Okt)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Okt)
                            </td>
                    }

                    if (item.Nov != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Ноябрь">
                                @Html.DisplayFor(modelItem => item.Nov)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Nov)
                            </td>
                    }

                    if (item.Dec != null)
                    {
                            <td data-bs-toggle="tooltip" data-bs-placement="top" title="@item.FullName Декабрь">
                                @Html.DisplayFor(modelItem => item.Dec)
                            </td>
                    }
                    else
                    {
                            <td>
                                @Html.DisplayFor(modelItem => item.Dec)
                            </td>
                    }
                }

        <td>
            @Html.DisplayFor(modelItem => item.Summ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VolPercent)%
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DivPercent)%
        </td>
        <!--<td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>-->
    </tr>}
</tbody>
</table>

            @{
    if (ViewData["Navigation"]!=null)
    {
        if (ViewData.Values !=null && ViewData.Values.Count()>0)
        {
            foreach (var item in ViewData["Navigation"] as List<int>)
            {
                                <a asp-action="Index" asp-route-year="@item" class="btn btn-default" style="border-color: #ddd;">@item</a>
            }
        }
    }
            }

</div>